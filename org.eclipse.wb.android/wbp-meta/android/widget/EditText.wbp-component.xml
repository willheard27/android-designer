<?xml version="1.0" encoding="UTF-8"?>
<component xmlns="http://www.eclipse.org/wb/WBPComponent">
	<description>EditText is a thin veneer over TextView that configures itself to be editable.</description>
	<!-- CREATION -->
	<creation>
		<source></source>
		<x-attribute name="android:text" value="New EditText"/>
	</creation>
	<!-- MORPHING -->
	<morphTargets>
		<noInherit/>
		<morphTarget class="android.widget.TextView"/>
		<morphTarget class="android.widget.CheckedTextView"/>
		<morphTarget class="android.widget.AutoCompleteTextView"/>
		<morphTarget class="android.widget.MultiAutoCompleteTextView"/>
		<morphTarget class="android.widget.Button"/>
	</morphTargets>
</component>
