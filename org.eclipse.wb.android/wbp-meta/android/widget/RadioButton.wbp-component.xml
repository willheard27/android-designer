<?xml version="1.0" encoding="UTF-8"?>
<component xmlns="http://www.eclipse.org/wb/WBPComponent">
	<description>A radio button is a two-states button that can be either checked or unchecked.</description>
	<!-- CREATION -->
	<creation>
		<source></source>
		<x-attribute name="android:text" value="New RadioButton"/>
	</creation>
	<!-- MORPHING -->
	<morphTargets>
		<noInherit/>
		<morphTarget class="android.widget.Button"/>
		<morphTarget class="android.widget.ToggleButton"/>
		<morphTarget class="android.widget.CheckBox"/>
		<morphTarget class="android.widget.TextView"/>
	</morphTargets>
</component>
