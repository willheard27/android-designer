	<!-- CREATION -->
	<creation>
		<source></source>
	</creation>
	<!-- PROPERTIES -->
	<properties-preferred names="android:textOn android:textOff"/>
	<properties-hidden names="android:text"/>
	<property-tag name="android:text" tag="isText" value="false"/>
</component>
