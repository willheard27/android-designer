<?xml version="1.0" encoding="UTF-8"?>
<component xmlns="http://www.eclipse.org/wb/WBPComponent">
	<description>An extension to TextView that supports the Checkable interface.</description>
	<!-- CREATION -->
	<creation>
		<source></source>
		<x-attribute name="android:text" value="New CheckedTextView"/>
	</creation>
	<!-- MORPHING -->
	<morphTargets>
		<noInherit/>
		<morphTarget class="android.widget.TextView"/>
		<morphTarget class="android.widget.EditText"/>
		<morphTarget class="android.widget.AutoCompleteTextView"/>
		<morphTarget class="android.widget.MultiAutoCompleteTextView"/>
		<morphTarget class="android.widget.CheckBox"/>
	</morphTargets>
</component>
